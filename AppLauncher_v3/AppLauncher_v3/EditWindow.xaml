<Window x:Class="AppLauncher_v3.EditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppLauncher_v3"
        mc:Ignorable="d"
        Title="EditWindow" Height="158.441" Width="300">
    <Grid Margin="0,0,0,21">

        <Button x:Name="button_OK" Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="33,69,0,0" Click="button_OK_Click"/>
        <Button x:Name="button_CANCEL" Content="CANCEL" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="185,69,0,0" Click="button_CANCEL_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="33,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="227"/>

    </Grid>
</Window>
